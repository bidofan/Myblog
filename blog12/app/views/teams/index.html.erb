<h1>List of teams</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Text</th>
    <th colspan="2"></th>
  </tr>
 
  <% @teams.each do |team| %>
    <tr>
      <td><%= team.name %></td>
      <td><%= team.description %></td>
      <td><%= link_to 'Show', team_path(team) %></td>
      <td><%= link_to 'Edit', team_path(team) %></td>
      <td><%= link_to 'Destroy', team_path(team), method: :delete,data: { confirm: 'Are you sure?' } %></td>



    </tr>
  <% end %>
</table>
<%= link_to 'New team', new_team_path %>
<%= link_to 'Back', welcome_index_path %>